n        setMobileNav(!mobileNav)\n      }>\n        {mobileNav ? (\n            <HiOutlineX className=\"text-3xl text-accent\"/>\n        ) : (\n            <HiMenuAlt4 className=\"text-3xl text-accent\"/>\n        )}\n      </button>\n    {/*  mobile nav - hidden on desktop */}\n      <div className={`${mobileNav? 'left-0' : 'left-full'} fixed top-0 bottom-0 w-[60vw] lg:hidden transition-all`}>\n        <MobileNav/>\n      </div>\n    </div>\n  </header>;\n};\n\nexport default Header;\n","/Users/ducminh/code/xpense-starter/src/components/Feature3.js",[],[],"/Users/ducminh/code/xpense-starter/src/components/Hero.js",[],[],"/Users/ducminh/code/xpense-starter/src/components/Product.js",[],[],"/Users/ducminh/code/xpense-starter/src/components/Pricing.js",["108"],[],"import React, {useState} from 'react';\n//import data\nimport {pricing} from '../data'\n//import icon\nimport {HiCheck, HiOutlineArrowNarrowRight} from 'react-icons/hi';\n\nconst Pricing = () => {\n  //index state\n  const [index, setIndex] = useState(1);\n  // pricing\n  const {title, cards} = pricing;\n  return <section className=\" section\">\n    <div className=\"container mx-auto\">\n      {/* title */}\n      <h2 className=\"h-2 mb-10 lg:mb-20 text-center\"\n          data-aos=\"fade-up\"\n          data-aos-delay=\"200\">{title}</h2>\n      {/* cards */}\n      <div className=\"flex flex-col lg:flex-row lg:gap-x-[30px] gap-y-[30px] lg:gap-y-0 justify-center items-center\">\n        {cards.map((card, cardIndex)=>{\n          const {icon, title, services, price, userAmount, btnText, delay} = card;\n          return (\n              <div data-aos=\"fade-up\"\n                   data-aos-delay=\"delay\"\n                   data-aos-offset=\"300\"\n                   key={cardIndex}>\n            <div onClick={()=> setIndex(cardIndex)} className={`${cardIndex === index ? 'bg-white shadow-2xl' : 'border' +\n                ' border-gray'} w-[350px] h-[500px] rounded-[12px] p-[40px] cursor-pointer transition-all`}>\n            {/*  card icon */}\n              <div className=\"mb-8\">\n                <img src={icon} alt=\"\"/>\n              </div>\n            {/* card title */}\n              <div className=\"text-[32px] font-semibold mb-8\">{title}</div>\n            {/* card service */}\n              <div className=\"flex flex-col gap-y-2 mb-6\">\n                {services.map((service, index)=>{\n                  //service\n                  const {name} = service;\n                  return <div className=\"flex items-center gap-x-[10px]\" key={index}>\n                    <HiCheck className=\"text-light\"/>\n                    <div>\n                      {name}\n                    </div>\n                  </div>\n                })}\n              </div>\n              <div className=\"mb-10\">\n                <div >\n                  <span className=\"text-2xl font-semibold\">{price}</span>\n                  <span className=\"text-xl text-light font-light\">year</span>\n                </div>\n                <div className=\"text-base text-light\">{userAmount}</div>\n              </div>\n            {/* btn */}\n              <button className={`${cardIndex === index ? 'bg-accent hover:bg-accent text-white' : 'border' +\n                  ' border-accent text-accent'} btn btn-sm space-x-[14px]`}>\n                <span>{btnText}</span>\n                <HiOutlineArrowNarrowRight/>\n              </button>\n            </div>\n          </div>)\n        })}\n      </div>\n    </div>\n  </section>;\n};\n\nexport default Pricing;\n","/Users/ducminh/code/xpense-starter/src/components/Testimonials.js",[],[],"/Users/ducminh/code/xpense-starter/src/components/Footer.js",["109"],[],"import React from 'react';\n//import data\nimport {footer} from '../data';\n// import components\nimport Copyright from '../components/Copyright';\n\nconst Footer = () => {\n  //footer data\n  const {logo, links, legal, newsletter, form } = footer;\n  return (\n    <footer className=\"pt-[142px] pb-[60px]\">\n    <div className=\"container mx-auto\">\n      <div className=\"flex flex-col items-center text-center lg:flex-row lg:items-start lg:text-left lg:justify-between gap-y-8\">\n      {/*  logo */}\n        <div data-aos=\"fade-up\"\n             data-aos-offset=\"200\"\n             data-aos-delay=\"300\">\n          <img src={logo} alt=\"\"/>\n        </div>\n      {/*  list 1 */}\n        <div data-aos=\"fade-up\"\n             data-aos-offset=\"200\"\n             data-aos-delay=\"500\">\n          <div className=\"text-2xl uppercase font-medium mb-6\">Links</div>\n          <ul className=\"flex flex-col gap-y-3\">\n            {links.map((item, index)=>{\n              // destructure item\n              const {href, name} = item\n              return <li key={index}>\n                <a className=\"font-medium hover:text-accent transition\" href={href}>{name}</a>\n              </li>\n            })}\n          </ul>\n        </div>\n      {/*  list 2 */}\n        <div data-aos=\"fade-up\"\n             data-aos-offset=\"200\"\n             data-aos-delay=\"700\">\n          <div className=\"text-2xl uppercase font-medium mb-6\">Legal</div>\n          <ul className=\"flex flex-col gap-y-3\">\n            {links.map((item, index)=>{\n              // destructure item\n              const {href, name} = item\n              return <li key={index}>\n                <a className=\"font-medium hover:text-accent transition\" href={href}>{name}</a>\n              </li>\n            })}\n          </ul>\n        </div>\n      {/*  newsletter */}\n        <div data-aos=\"fade-up\"\n             data-aos-offset=\"200\"\n             data-aos-delay=\"900\">\n          <div className=\"text-2xl uppercase font-medium mb-6\">{newsletter.title}</div>\n          <div className=\"text-xl text-light mb-[18px]\">{newsletter.subtitle}</div>\n        {/* form */}\n          <form className=\" max-w-[349px] mb-[10px]\">\n            <div className=\"h-[62px] p-[7px] flex border border-dark rounded-lg\">\n              <input className=\"w-full h-full pl-6 border-none outline-none placeholder:text-dark\" type=\"text\" placeholder={form.placeholder}/>\n              <button className=\"btn btn-sm bg-accent hover:bg-accentHover w-[102px] text-white\">{form.btnText}</button>\n            </div>\n          </form>\n          <span className=\"text-sm text-light\" >{form.smallText}</span>\n        </div>\n      </div>\n      <hr className=\"mt-10 mb-5\"\n          data-aos=\"fade-up\"\n          data-aos-offset=\"100\"\n          data-aos-delay=\"200\"/>\n      <Copyright/>\n    </div>\n  </footer>\n      )\n};\n\nexport default Footer;\n",[],"/Users/ducminh/code/xpense-starter/src/components/Cta.js",[],[],"/Users/ducminh/code/xpense-starter/src/data.js",[],[],"/Users/ducminh/code/xpense-starter/src/components/MobileNav.js",[],[],"/Users/ducminh/code/xpense-starter/src/components/Nav.js",[],[],"/Users/ducminh/code/xpense-starter/src/components/Cards.js",["110","111"],[],"import React,{useState} from 'react';\n//import data\nimport {product} from '../data';\n//import image\nimport ArrowImg from '../assets/img/product/cards/arrow.svg'\n\nconst Cards = () => {\n  //index state\n  const [index, setIndex] = useState(1);\n  //product data\n  const {cards} = product;\n  return <div>\n  {/*  cards */}\n    <div className=\"flex flex-col gap-y-[30px] lg:flex-row lg:gap-x-[30px]\">\n      {cards.map((card, cardIndex) => {\n        const {title, subtitle, icon, delay} = card;\n        return <div key={cardIndex}\n                    data-aos=\"zoom-out\"\n                    data-aos-offset=\"300\"\n                    data-aos-delay=\"delay\">\n          <div onClick={() => setIndex(cardIndex)}\n              className={`${index === cardIndex && 'bg-white shadow-2xl'} w-[350px] h-[350px] flex flex-col justify-center mx-auto p-[65px]' +\n              ' text-center rounded-[12px] cursor-pointer transition-all items-center`}>\n          {/* card icon */}\n            <div className=\"mb-6\">\n              <img src={icon} alt=\"\"/>\n            </div>\n            {/* card title */}\n            <div className=\"mb-3 text-[30px] font-medium \">{title}</div>\n            {/* card subtitle */}\n            <p className=\"mb-6 text-light\">{subtitle}</p>\n            {/* arrow image */}\n            {index === cardIndex && <img src={ArrowImg}/> }\n          </div>\n        </div>;\n      })}\n    </div>\n  </div>;\n};\n\nexport default Cards;\n","/Users/ducminh/code/xpense-starter/src/components/ClientSlider.js",[],[],"/Users/ducminh/code/xpense-starter/src/components/Copyright.js",[],[],{"ruleId":"112","severity":1,"message":"113","line":25,"column":7,"nodeType":"114","endLine":25,"endColumn":62},{"ruleId":"115","severity":1,"message":"116","line":21,"column":69,"nodeType":"117","messageId":"118","endLine":21,"endColumn":74},{"ruleId":"115","severity":1,"message":"119","line":9,"column":23,"nodeType":"117","messageId":"118","endLine":9,"endColumn":28},{"ruleId":"115","severity":1,"message":"116","line":16,"column":39,"nodeType":"117","messageId":"118","endLine":16,"endColumn":44},{"ruleId":"120","severity":1,"message":"121","line":33,"column":37,"nodeType":"114","endLine":33,"endColumn":58},"jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/jsx-eslint/eslint-plugin-jsx-a11y/blob/HEAD/docs/rules/anchor-is-valid.md","JSXOpeningElement","no-unused-vars","'delay' is assigned a value but never used.","Identifier","unusedVar","'legal' is assigned a value but never used.","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images."]